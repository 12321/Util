@page
@model FormModel
<page-header title="基础表单">
    将一个冗长或用户不熟悉的表单任务分成多个步骤，指导用户完成。
</page-header>
<nz-card>
    <util-form id="form" on-submit="submit(form)">
        <nz-form-item>
            <nz-form-label nzFor="name">姓名</nz-form-label>
            <util-textbox for="Name"></util-textbox>
        </nz-form-item>
        <nz-form-item>
            <util-select url="/api/form/groupItems" multiple="true" width="500"></util-select>
        </nz-form-item>
        <nz-form-item>
            <util-select for="Nation"></util-select>
        </nz-form-item>
        <nz-form-item>
            <util-date-picker for="DateTime"></util-date-picker>
        </nz-form-item>
        <nz-form-item>
            <util-textarea for="Comment"></util-textarea>
        </nz-form-item>
        <nz-form-item>
            <util-editor for="Comment"></util-editor>
        </nz-form-item>
        <nz-form-item>
            <nz-form-control>
                <util-button is-submit="true" color="Primary"></util-button>
            </nz-form-control>
        </nz-form-item>
    </util-form>
</nz-card>