@page
@model FormDto
<page-header title="基础表单">
    将一个冗长或用户不熟悉的表单任务分成多个步骤，指导用户完成。
</page-header>
<nz-card>
    <util-form id="form" on-submit="submit(form)">
        <nz-form-item>
            <nz-form-label  nzFor="email">E-mail</nz-form-label>
            <util-textbox for="Name"></util-textbox>
        </nz-form-item >
        <nz-form-item>
            <nz-form-control>
                <util-button is-submit="true" color="Primary"></util-button>
            </nz-form-control>
        </nz-form-item >
    </util-form>
</nz-card>

